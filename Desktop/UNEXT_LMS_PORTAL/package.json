    "ng2-date-picker": "^12.0.4",
    "ng2-opd-popup": "^1.1.21",
    "ng7-bootstrap-breadcrumb": "^6.0.0",
    "ngx-doc-viewer": "^2.0.5",
    "ngx-extended-pdf-viewer": "^10.0.0",
