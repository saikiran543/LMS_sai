    "rxjs": "^7.3.1",
    "sweetalert2": "^11.0.16",
    "tslib": "^2.1.0",
    "uuid": "^8.3.2",
    "xlsx": "^0.18.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^12.2.5",
    "@angular/cli": "~12.0.1",
    "@angular/compiler-cli": "~12.0.1",
    "@types/jasmine": "~3.6.0",
    "@types/jquery": "^3.5.13",
    "@types/lodash": "^4.14.177",
    "@types/node": "^12.11.1",
    "@types/pdfmake": "^0.1.20",
    "@types/uuid": "^8.3.1",
    "@typescript-eslint/eslint-plugin": "^4.28.0",
    "@typescript-eslint/parser": "^4.28.0",
    "del-cli": "^3.0.1",
    "eslint": "^7.29.0",
    "husky": "^6.0.0",
    "jasmine-core": "~3.7.0",
    "karma": "~6.3.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.0.3",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "lint-staged": "^11.0.1",
    "typescript": "~4.2.3"
  },
  "lint-staged": {
    "src/**/*.ts": "eslint --max-warnings=0"
  }
