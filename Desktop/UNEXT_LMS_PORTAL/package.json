{
  "name": "lms-portal",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --proxy-config proxy.conf.json",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "build:edunxtdev01": "ng build --configuration=production --output-hashing=all --source-map=false --build-optimizer=true --aot=true --optimization=true",
    "deploy:edunxtdev01": "npm run build:edunxtdev01 && npm run upload-to-s3:edunxtdev01 && npm run upload-index-html:edunxtdev01 && npm run clear-cloudfront-cache:edunxtdev01",
    "upload-to-s3:edunxtdev01": "aws s3 sync dist/lms-portal/ s3://edunxt-new-portal/dev/edunxtdev01/ng/ --delete --cache-control \"max-age=15552000\"",
    "upload-index-html:edunxtdev01": "aws s3 cp dist/lms-portal/index.html s3://edunxt-new-portal/dev/edunxtdev01/ng/ --cache-control \"no-cache\"",
    "clear-cloudfront-cache:edunxtdev01": "aws cloudfront create-invalidation --distribution-id E3DCEXQ45JLVDJ --paths \"/*\"",
    "build:edunxtqa01": "ng build --configuration=production --output-hashing=all --source-map=false --build-optimizer=true --aot=true --optimization=true",
    "deploy:edunxtqa01": "npm run build:edunxtqa01 && npm run upload-to-s3:edunxtqa01 && npm run upload-index-html:edunxtqa01 && npm run clear-cloudfront-cache:edunxtqa01",
    "upload-to-s3:edunxtqa01": "aws s3 sync dist/lms-portal/ s3://edunxt-new-portal/qa/edunxtqa01/ng/ --delete --cache-control \"max-age=15552000\"",
    "upload-index-html:edunxtqa01": "aws s3 cp dist/lms-portal/index.html s3://edunxt-new-portal/qa/edunxtqa01/ng/ --cache-control \"no-cache\"",
    "clear-cloudfront-cache:edunxtqa01": "aws cloudfront create-invalidation --distribution-id E2A5BIQXXIGLZ7 --paths \"/*\"",
    "build:edunxtuat01": "ng build --configuration=production --output-hashing=all --source-map=false --build-optimizer=true --aot=true --optimization=true",
    "deploy:edunxtuat01": "npm run build:edunxtuat01 && npm run upload-to-s3:edunxtuat01 && npm run upload-index-html:edunxtuat01 && npm run clear-cloudfront-cache:edunxtuat01",
    "upload-to-s3:edunxtuat01": "aws s3 sync dist/lms-portal/ s3://edunxt-new-portal/uat/edunxtuat01/ng/ --delete --cache-control \"max-age=15552000\"",
    "upload-index-html:edunxtuat01": "aws s3 cp dist/lms-portal/index.html s3://edunxt-new-portal/uat/edunxtuat01/ng/ --cache-control \"no-cache\"",
    "clear-cloudfront-cache:edunxtuat01": "aws cloudfront create-invalidation --distribution-id E19LGPORUGNPCA --paths \"/*\"",
    "coverage": "ng test --code-coverage",
    "prepare": "npm audit --audit-level critical && husky install && husky set .husky/pre-commit \"npx lint-staged\""
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^12.0.5",
    "@angular/cdk": "^12.2.2",
    "@angular/common": "~12.0.1",
    "@angular/compiler": "~12.0.1",
    "@angular/core": "~12.0.1",
    "@angular/forms": "~12.0.1",
    "@angular/material": "^12.2.2",
    "@angular/platform-browser": "~12.0.1",
    "@angular/platform-browser-dynamic": "~12.0.1",
    "@angular/router": "~12.0.1",
    "@auth0/angular-jwt": "^5.0.2",
    "@ckeditor/ckeditor5-angular": "^2.0.2",
    "@edunxtv2/unext-ck-editor": "^1.0.1",
    "@edunxtv2/unext-epub-viewer": "^1.1.0",
    "@ng-bootstrap/ng-bootstrap": "^10.0.0",
    "@ngx-translate/core": "^13.0.0",
