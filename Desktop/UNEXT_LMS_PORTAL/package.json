    "angular-svg-icon": "^12.0.0",
    "angularx-flatpickr": "^6.6.0",
    "bootstrap": "^5.0.2",
    "bootstrap-slider": "^11.0.2",
    "calendar-utils": "0.9.0",
    "cropperjs": "^1.5.12",
    "date-fns": "^2.28.0",
    "epubjs": "^0.2.21",
    "flatpickr": "4.6.9",
